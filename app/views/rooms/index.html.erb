<h1>Rooms#index</h1>
<p>Find me in app/views/rooms/index.html.erb</p>

<% @rooms.each do |room| %>
  <%= link_to room_path(room), class: "dm_list__content__link" do %>
    <h3 class="dm_list__content__link__box__name">
      <%= opponent_user(room) %>
    </h3>
    <p class="dm_list__content__link__box__message">
      <%= most_new_message_preview(room) %>
    </p>
  <% end %>
<% end %>